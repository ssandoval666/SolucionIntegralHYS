{
  "openapi": "3.0.1",
  "info": {
    "title": "Demo API",
    "version": "v1"
  },
  "paths": {
    "/api/v{version}/Settings/Setting": {
      "get": {
        "tags": [
          "Settings"
        ],
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/SmartSettings"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SmartSettings"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/SmartSettings"
                }
              }
            }
          }
        }
      }
    },
    "/api/v{version}/JWT/Token": {
      "post": {
        "tags": [
          "Token"
        ],
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/JWTInfo"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/JWTInfo"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/JWTInfo"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/v{version}/Usuario/Validar": {
      "get": {
        "tags": [
          "Usuario"
        ],
        "parameters": [
          {
            "name": "sUserName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sPassword",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/Usuario"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Usuario"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/Usuario"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "JWTInfo": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SmartSettings": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "version": {
            "type": "string",
            "nullable": true
          },
          "app": {
            "type": "string",
            "nullable": true
          },
          "appName": {
            "type": "string",
            "nullable": true
          },
          "appFlavor": {
            "type": "string",
            "nullable": true
          },
          "appFlavorSubscript": {
            "type": "string",
            "nullable": true
          },
          "logo": {
            "type": "string",
            "nullable": true
          },
          "activo": {
            "type": "boolean"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Usuario": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "nombre": {
            "type": "string",
            "nullable": true
          },
          "apellido": {
            "type": "string",
            "nullable": true
          },
          "foto": {
            "type": "string",
            "nullable": true
          },
          "fechaIngreso": {
            "type": "string",
            "format": "date-time"
          },
          "fechaEgreso": {
            "type": "string",
            "format": "date-time"
          },
          "capacitaciones": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UsuarioCapacitacion"
            },
            "nullable": true
          },
          "idPerfil": {
            "type": "integer",
            "format": "int32"
          },
          "activo": {
            "type": "boolean"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "UsuarioCapacitacion": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "usuarioId": {
            "type": "integer",
            "format": "int32"
          },
          "capacitacionId": {
            "type": "integer",
            "format": "int32"
          },
          "realizoCurso": {
            "type": "boolean"
          },
          "feachadeRealizacion": {
            "type": "string",
            "format": "date-time"
          },
          "activo": {
            "type": "boolean"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Please enter a valid token",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}